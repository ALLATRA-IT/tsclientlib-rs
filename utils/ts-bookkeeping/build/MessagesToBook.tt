								<# }
								RuleOp::Remove => { #>
				let val = cmd.get("<#= from.ts #>").ok_or_else(|| format_err!(
						"<#= from.ts #> not found but is not optional in <#= enum_name #>"))?;
				let val = { <#= generate_deserializer(from) #> };
