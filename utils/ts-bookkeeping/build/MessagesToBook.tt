									<# } else if from_opt { #>
				if let Some(val) = val {
									<# if to.opt { #>
					let old = mem::replace(&mut r.<#= to_snake_case(&to.name) #>, Some(val));
									<# } else { #>
					let old = mem::replace(&mut r.<#= to_snake_case(&to.name) #>, val);
									<# } #>
				<# if from.modifier.as_ref().map(|s| s == "array").unwrap_or(false) {
